   "source": [
    "Program to print the fibonacci series upto N."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "d46fb811",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter a number : 6\n",
      "Fibonacci series upto 6\n",
      "0 1 1 2 3 5 "
     ]
    }
   ],
   "source": [
    "num = int(input(\"Enter a number : \"))\n",
    "\n",
    "n1, n2 = 0, 1\n",
    "\n",
    "print(\"Fibonacci series upto\",num)\n",
    "print(n1,end=' ')\n",
    "print(n2,end=' ')\n",
    "\n",
    "for i in range(2,num) :\n",
    "    fib = n1 + n2\n",
    "    print(fib, end=' ')\n",
    "    n1, n2 = n2, fib"
   ]
  }
