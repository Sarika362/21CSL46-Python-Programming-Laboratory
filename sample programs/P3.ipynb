   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter a number : 10\n",
      "Sum of 10 natural numbers = 55\n"
     ]
    }
   ],
   "source": [
    "num = int(input(\"Enter a number : \"))\n",
    "\n",
    "sum = 0\n",
    "for i in range(1, num+1) :\n",
    "    sum += i\n",
    "    \n",
    "print(f'Sum of {num} natural numbers = {sum}')"
   ]
  }
